<?xml version="1.0" encoding="UTF-8"?>
<!-- Crea un XML que represente un **instituto**.  
Debe tener varias `clases`, y cada clase varios `alumnos`.  
Los alumnos deben tener al menos: `nombre`, `edad` y `notaMedia`.-->

<instituto>
    <clase>
        <alumno>
            <nombre>Juan</nombre>
            <edad>18</edad>
            <notaMedia>8,7</notaMedia>
        </alumno>
        <alumno>
            <nombre>Reda</nombre>
            <edad>21</edad>
            <notaMedia>10</notaMedia>
        </alumno>
    </clase>

    <clase>
        <alumno>
            <nombre>Sem</nombre>
            <edad>20</edad>
            <notaMedia>3,1</notaMedia>
        </alumno>
        <alumno>
            <nombre>Oliver</nombre>
            <edad>21</edad>
            <notaMedia>4</notaMedia>
        </alumno>
    </clase>
</instituto>